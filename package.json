{
  "name": "gravyprompts",
  "version": "0.1.0",
  "private": true,
  "description": "Store, share, populate, and rate your AI prompt templates. The ultimate prompt management platform for AI enthusiasts and professionals.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "node scripts/setup-gravyjs.js",
    "dev": "echo 'Please use npm run dev:all for local development' && exit 1",
    "dev:all": "./scripts/dev-all.sh",
    "dev:all:empty": "./scripts/dev-all-empty.sh",
    "build": "next build",
    "build:all": "npm run build && cd packages/gravyjs && npm run build",
    "start": "next start -p 6827",
    "start:next": "next start -p 6827",
    "start:demo": "cd packages/gravyjs-demo && npm run dev",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --config jest.config.ci.js --ci --coverage --maxWorkers=2",
    "test:ci:components": "jest --config jest.config.ci.js components/__tests__ --ci",
    "test:local": "jest __tests__/security __tests__/e2e __tests__/integration",
    "test:e2e": "jest __tests__/e2e --runInBand",
    "test:smoke:staging": "jest __tests__/deployment/smoke-tests.test.js --testNamePattern='staging'",
    "test:smoke:production": "jest __tests__/deployment/smoke-tests.test.js --testNamePattern='production'",
    "build:cdk": "cd cdk && rm -f lib/*.d.ts lib/*.js && npm run build",
    "cdk:install": "cd cdk && npm install",
    "cdk:synth": "cd cdk && npx cdk synth",
    "deploy:cert:first": "./scripts/deploy-cert-first.sh",
    "check:cert": "./scripts/check-certificate.sh",
    "deploy:waf": "./scripts/deploy-waf.sh",
    "deploy:auth": "./scripts/deploy-auth.sh",
    "validate:auth": "./scripts/validate-auth-setup.sh",
    "deploy:api": "./scripts/deploy-api.sh",
    "deploy:budget": "./scripts/deploy-budget.sh",
    "deploy:dashboard": "./scripts/deploy-dashboard.sh",
    "deploy:all": "./scripts/deploy-all-amplify.sh",
    "deploy:backend": "./scripts/deploy-amplify-backend.sh",
    "deploy:cache": "node ./scripts/deploy-cache.js",
    "status": "./scripts/check-stack-status.sh",
    "check:budget": "./scripts/check-budget-status.sh",
    "local:install": "cd cdk/local-test && npm install",
    "local:setup": "cd cdk/local-test && docker-compose up -d && sleep 5 && npm install && node setup-local-db.js",
    "local:start": "cd cdk/local-test && ./run-local.sh",
    "local:tables": "cd cdk/local-test && node setup-local-db.js",
    "local:api": "cd cdk/local-test && ./run-local.sh",
    "local:stop": "cd cdk/local-test && docker-compose down",
    "local:setup:admin": "node scripts/local/setup-admin-permissions.js",
    "local:logs": "docker logs gravyprompts-dynamodb-local",
    "local:cleanup": "./scripts/cleanup-local.sh",
    "local:test:cache": "node ./scripts/test-redis-cache.js",
    "debug:auth": "node scripts/debug-auth.js",
    "test:backend": "cd cdk && npm test",
    "test:backend:watch": "cd cdk && npm run test:watch",
    "test:backend:coverage": "cd cdk && npm run test:coverage",
    "verify:local": "node scripts/verify-local-config.js",
    "fix:local-config": "node scripts/fix-local-config.js",
    "analyze:costs": "node scripts/analyze-costs.js",
    "local:clear": "node scripts/clear-local-db.js",
    "local:clear:all": "node scripts/clear-local-db.js",
    "local:test": "cd cdk/local-test && ./test-api.sh",
    "local:test:api": "./scripts/test-local-api.sh",
    "local:fix-auth": "./scripts/fix-local-auth.sh",
    "test:infra": "./scripts/test-infrastructure.sh",
    "pre-flight": "./scripts/pre-flight-check.sh",
    "status:all": "./scripts/check-all-stacks.sh",
    "destroy:waf": "./scripts/destroy-waf.sh",
    "destroy:monitoring": "./scripts/destroy-monitoring.sh",
    "destroy:all": "./scripts/destroy-all.sh",
    "amplify:setup": "echo 'Run AWS Amplify Console setup - see AMPLIFY_SETUP.md'",
    "deploy:force": "./scripts/force-update.sh",
    "todo": "./scripts/update-todos.sh show",
    "todo:add": "./scripts/update-todos.sh add",
    "todo:complete": "./scripts/update-todos.sh complete",
    "todo:progress": "./scripts/update-todos.sh progress",
    "demo:dev": "cd packages/gravyjs-demo && npm run dev",
    "demo:build": "cd packages/gravyjs-demo && npm run build",
    "demo:preview": "cd packages/gravyjs-demo && npm run preview",
    "demo:setup": "cd packages/gravyjs-demo && npm run setup",
    "demo:deploy": "cd packages/gravyjs-demo && npm run deploy",
    "demo:check:redirect": "cd packages/gravyjs-demo && npm run check:redirect",
    "demo:debug": "cd packages/gravyjs-demo && npm run debug",
    "check:amplify:app": "./scripts/check-amplify-app.sh",
    "check:amplify:dns": "./scripts/check-amplify-dns.sh",
    "check:amplify:domain": "./scripts/check-amplify-domain-status.sh",
    "diagnose:stack": "./scripts/diagnose-stack.sh",
    "protect:cert": "./scripts/protect-certificate.sh",
    "templates:load": "node scripts/bulk-load-templates.js",
    "templates:delete": "node scripts/bulk-delete-templates.js",
    "templates:analyze": "node scripts/analyze-markdown-templates.js",
    "templates:load:local": "node scripts/bulk-load-templates-local.js",
    "templates:consolidate": "python scripts/consolidate-templates.py",
    "check:templates": "./scripts/check-templates.sh",
    "deploy:cert": "./scripts/deploy-cert.sh",
    "cdk:env": "source ./scripts/cdk-env.sh && env | grep -E '^(AWS_|CDK_)'"
  },
  "dependencies": {
    "aws-amplify": "^6.15.6",
    "colors": "^1.4.0",
    "date-fns": "^4.1.0",
    "gravyjs": "*",
    "lucide-react": "^0.544.0",
    "next": "^15.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudwatch": "^3.893.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.888.0",
    "@aws-sdk/client-dynamodb": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.883.0",
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "aws-xray-sdk-core": "^3.10.3",
    "concurrently": "^9.1.2",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^16.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jest-axe": "^10.0.0",
    "license-checker": "^25.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "uuid": "^11.1.0"
  }
}
